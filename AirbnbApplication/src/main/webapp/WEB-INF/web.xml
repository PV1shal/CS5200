<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>AirbnbApplication</display-name>
  <welcome-file-list>
    <welcome-file>/home</welcome-file>
  </welcome-file-list>
   <servlet>
   		<servlet-name>FindHosts</servlet-name>
   		<servlet-class>airbnb.servlet.hosts.FindHosts</servlet-class>
   </servlet>
   <servlet>
		<servlet-name>FindListings</servlet-name>
		<servlet-class>airbnb.servlet.listings.HomeListings</servlet-class>
	</servlet>
   <servlet-mapping>
   <!-- 
   		<servlet-name>FindHosts</servlet-name>
   		<url-pattern>/home</url-pattern>
    -->
   		<servlet-name>FindListings</servlet-name>
   		<url-pattern>/home</url-pattern>
   </servlet-mapping>
</web-app>